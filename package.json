{
    "name": "envoy-cli",
    "version": "1.0.0",
    "description": "The envoy between your code and environment variable config - Never miss an environment variable again",
    "main": "dist/index.js",
    "bin": {
        "envoy-cli": "./dist/index.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "tsc && tsc-alias",
        "dev": "tsc --watch",
        "start": "node dist/index.js",
        "prepublishOnly": "npm run build",
        "prepack": "npm run build && npm run test",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "oxlint src --deny-warnings",
        "lint:fix": "oxlint src --fix",
        "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\""
    },
    "keywords": [
        "env",
        "environment",
        "dotenv",
        "cli",
        "devtools",
        "environment-variables",
        "env-management",
        "typescript",
        "validation",
        "sync",
        "configuration"
    ],
    "author": "LeDevNovice",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeDevNovice/envoy-cli.git"
    },
    "bugs": {
        "url": "https://github.com/LeDevNovice/envoy-cli/issues"
    },
    "homepage": "https://github.com/LeDevNovice/envoy-cli#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.6.2",
        "@vitest/coverage-v8": "^3.2.4",
        "oxlint": "^1.19.0",
        "prettier": "^3.6.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "chalk": "^5.6.2",
        "commander": "^14.0.1",
        "glob": "^11.0.3"
    }
}
